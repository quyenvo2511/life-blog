messages":"142","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"72"},"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/index.js",[],["143","144"],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/App.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/store.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/routers/AdminLayout.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/components/AlertMsg.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/routers/PublicLayout.js",["145"],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/routers/PrivateRoute.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/reducers/index.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/components/PublicNavbar.js",["146"],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/NotFoundPage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/AddEditBlogPage.js",["147"],"import React, { useState, useEffect } from \"react\";\nimport { Button, Form, FormGroup } from \"react-bootstrap\";\nimport { ClipLoader } from \"react-spinners\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { useHistory, useParams } from \"react-router-dom\";\n\nimport routeActions from \"../redux/actions/route.actions\";\nimport blogActions from \"../redux/actions/blog.actions\";\n\nconst AddEditBlogPage = () => {\n  const [formData, setFormData] = useState({\n    title: \"\",\n    content: \"\",\n    images: [],\n  });\n  const userLoading = useSelector((state) => state.auth.Loading);\n  const blog = useSelector((state) => state.blog.selectedBlog);\n  const accessToken = useSelector((state) => state.auth.accessToken);\n  const redirectTo = useSelector((state) => state.route.redirectTo);\n  const blogLoading = useSelector((state) => state.blog.loading);\n\n  const params = useParams();\n  const blogId = params.id;\n  const dispatch = useDispatch();\n  const history = useHistory();\n\n  useEffect(() => {\n    if (redirectTo) {\n      history.push(redirectTo);\n      dispatch(routeActions.removeRedirectTo());\n    }\n  }, [dispatch, history, redirectTo]);\n\n  useEffect(() => {\n    if (blogId) {\n      dispatch(blogActions.blogRequest(blogId));\n    } else {\n      setFormData({ title: \"\", content: \"\", images: [] });\n    }\n  }, []);\n\n  useEffect(() => {\n    if (blog) {\n      setFormData({\n        title: blog.title,\n        content: blog.content,\n        images: blog.images,\n      });\n    }\n  }, [blog]);\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    const { title, content, images } = formData;\n    dispatch(blogActions.writeBlogRequest(title, content, images, accessToken));\n  };\n  const handleChange = (e) => {\n    setFormData({ ...formData, [e.target.name]: e.target.value });\n  };\n  const handleDelete = () => {\n    dispatch(blogActions.deleteBlogRequest(blogId, accessToken));\n  };\n\n  const handleCancelEditing = () => {\n    dispatch(routeActions.redirect(`/blogs/${blogId}`));\n  };\n  const handleApplyChange = () => {\n    const { title, content, images } = formData;\n    dispatch(\n      blogActions.updateBlogRequest(title, content, images, accessToken, blogId)\n    );\n  };\n  return userLoading || blogLoading ? (\n    <div>\n      <ClipLoader color=\"red\" size={150} loading={true} />\n    </div>\n  ) : (\n    <div>\n      <Form onSubmit={handleSubmit}>\n        <FormGroup>\n          <Form.Control\n            placeholder=\"Title\"\n            value={formData.title}\n            name=\"title\"\n            onChange={handleChange}\n          />\n        </FormGroup>\n        <FormGroup>\n          <Form.Control\n            placeholder=\"Content\"\n            value={formData.content}\n            name=\"content\"\n            onChange={handleChange}\n          />\n        </FormGroup>\n        {blogId ? null : <Button type=\"submit\">Submit</Button>}\n      </Form>\n      {blogId ? (\n        <div style={{ marginTop: 20 }}>\n          <Button variant=\"success\" onClick={() => handleApplyChange()}>\n            Apply changes\n          </Button>\n          {\"  \"}\n          <Button variant=\"danger\" onClick={() => handleDelete()}>\n            Delete post\n          </Button>\n          {\"  \"}\n          <Button variant=\"light\" onClick={() => handleCancelEditing()}>\n            Cancel\n          </Button>\n        </div>\n      ) : null}\n    </div>\n  );\n};\n\nexport default AddEditBlogPage;\n","/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/BlogDetailPage.js",["148","149","150"],"import React, { useEffect, useState } from \"react\";\nimport { useParams, useHistory } from \"react-router-dom\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { ClipLoader } from \"react-spinners\";\nimport routeActions from \"../redux/actions/route.actions\";\nimport blogActions from \"../redux/actions/blog.actions\";\nimport userActions from \"../redux/actions/user.actions\";\nimport { Button, Form, FormGroup, Row, Col } from \"react-bootstrap\";\nimport api from \"../apiService\";\nimport { ACCESS_TOKEN } from \"../redux/constants/auth.constants\";\nimport ReviewCard from \"../components/ReviewCard\";\nimport { toast } from \"react-toastify\";\n\nconst BlogDetailPage = () => {\n  const params = useParams();\n  const [review, setReview] = useState(\"\");\n  const [reviews, setReviews] = useState([]);\n  const blog = useSelector((state) => state.blog.selectedBlog);\n  const users = useSelector((state) => state.user.users);\n  const userId = useSelector((state) => state.auth.user._id);\n  const redirectTo = useSelector((state) => state.route.redirectTo);\n  const blogLoading = useSelector((state) => state.blog.blogLoading);\n  const usersLoading = useSelector((state) => state.user.loading);\n\n  const blogId = params.id;\n\n  const history = useHistory();\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    if (redirectTo) {\n      history.push(redirectTo);\n      dispatch(routeActions.removeRedirectTo());\n    }\n  }, [dispatch, history, redirectTo]);\n\n  useEffect(() => {\n    const accessToken = sessionStorage.getItem(ACCESS_TOKEN);\n    dispatch(blogActions.blogRequest(params.id));\n    dispatch(userActions.getUsersRequest(accessToken));\n  }, [dispatch]);\n\n  useEffect(() => {\n    const getAllReviews = async () => {\n      try {\n        const res = await api.get(`/api/reviews/blogs/${blogId}`);\n        setReviews(res.data.data.reviews);\n      } catch (error) {\n        toast.error(\"Cannot get reviews of this post\");\n      }\n    };\n\n    getAllReviews();\n  }, []);\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    const postReview = async () => {\n      try {\n        const accessToken = sessionStorage.getItem(ACCESS_TOKEN);\n        const body = { content: review };\n        const header = { Authorization: `Bearer ${accessToken}` };\n        const res = await api.post(`/api/reviews/blogs/${blogId}`, body, {\n          headers: header,\n        });\n        history.go(0);\n      } catch (error) {\n        toast.error(\"Error when posting your review :(\");\n      }\n    };\n    postReview();\n  };\n\n  const handleChange = (e) => {\n    setReview(e.target.value);\n  };\n\n  const handleEditClicked = () => {\n    dispatch(routeActions.redirect(`/admin/blog/edit/${blogId}`));\n  };\n\n  const ReviewList = () => {\n    const getUserName = (userId) => {\n      const name = users.filter((user) => user._id === userId)[0].name;\n      console.log(name);\n\n      return name;\n    };\n\n    return reviews.length > 0\n      ? reviews.map((review) => {\n          const name = getUserName(review.user);\n          return <ReviewCard author={name} content={review.content} />;\n        })\n      : null;\n  };\n  return blogLoading || usersLoading ? (\n    <div className=\"text-center\">\n      <ClipLoader color=\"red\" size={150} loading={true} />\n    </div>\n  ) : (\n    <div style={{ position: \"relative\" }}>\n      {blog?.author._id === userId ? (\n        <Button\n          style={{ position: \"absolute\", top: 20, right: 20 }}\n          onClick={handleEditClicked}\n        >\n          Edit\n        </Button>\n      ) : null}\n      <h2>{blog?.title}</h2>\n      <p>{blog?.content}</p>\n      <p\n        style={{ fontStyle: \"italic\", color: \"gray\" }}\n      >{`@${blog?.author.name}`}</p>\n      <ReviewList />\n      <Form onSubmit={handleSubmit}>\n        <FormGroup as={Row}>\n          <Form.Label column sm=\"2\">\n            Review:\n          </Form.Label>\n          <Col sm=\"8\">\n            <Form.Control\n              name=\"userName\"\n              placeholder=\"Write a review...\"\n              value={review}\n              onChange={handleChange}\n            />\n          </Col>\n          <Col sm=\"2\">\n            <Button type=\"submit\">Submit</Button>\n          </Col>\n        </FormGroup>\n      </Form>\n    </div>\n  );\n};\n\nexport default BlogDetailPage;\n","/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/HomePage.js",["151"],"import React, { useState, useEffect } from \"react\";\nimport { Button, Card, Container, CardDeck } from \"react-bootstrap\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport blogActions from \"../redux/actions/blog.actions\";\nimport routeActions from \"../redux/actions/route.actions\";\nimport { ClipLoader } from \"react-spinners\";\nimport PaginationBar from \"../components/PaginationBar\";\nimport moment from \"moment\";\nimport { useHistory } from \"react-router-dom\";\n\nconst HomePage = () => {\n  const blogs = useSelector((state) => state.blog.blogs);\n  const totalPageNum = useSelector((state) => state.blog.totalPageNum);\n  const loading = useSelector((state) => state.blog.loading);\n  const isAuthenticated = useSelector((state) => state.auth.isAuthenticated);\n  const [pageNum, setPageNum] = useState(1);\n  const redirectTo = useSelector((state) => state.route.redirectTo);\n  const dispatch = useDispatch();\n  const history = useHistory();\n  useEffect(() => {\n    dispatch(blogActions.blogsRequest(pageNum));\n  }, [dispatch, pageNum]);\n\n  useEffect(() => {\n    if (redirectTo) {\n      history.push(redirectTo);\n      dispatch(routeActions.removeRedirectTo());\n    }\n  }, [dispatch, redirectTo]);\n  const handleWriteBlog = () => {\n    dispatch(routeActions.redirect(`/blog/add`));\n  };\n  const handleCardClick = (blogId) => {\n    dispatch(routeActions.redirect(`/blogs/${blogId}`));\n  };\n  return (\n    <Container>\n      <Card className=\"text-center\">\n        <Card.Body>\n          <Card.Title>SOCIAL BLOG</Card.Title>\n          <Card.Text>Write about your amazing things</Card.Text>\n          {isAuthenticated ? (\n            <Button variant=\"primary\" onClick={handleWriteBlog}>\n              Write Blog\n            </Button>\n          ) : (\n            <></>\n          )}\n        </Card.Body>\n      </Card>\n      <PaginationBar\n        pageNum={pageNum}\n        setPageNum={setPageNum}\n        totalPageNum={totalPageNum}\n      ></PaginationBar>\n      {loading ? (\n        <div className=\"text-center\">\n          <ClipLoader color=\"red\" size={150} loading={true} />\n        </div>\n      ) : (\n        <>\n          {blogs.length > 0 ? (\n            <CardDeck>\n              {blogs.map((blog) => (\n                <Card\n                  className=\" mt-5 d-flex flex-wrapped\"\n                  style={{ width: \"200px\", flex: \"0 0 auto\", height: \"200px\" }}\n                  onClick={() => {\n                    handleCardClick(blog._id);\n                  }}\n                >\n                  {blog.images?.length > 0 && (\n                    <Card.Img variant=\"top\" src={blog.images[0]} />\n                  )}\n                  <Card.Body>\n                    <Card.Title>{blog.title}</Card.Title>\n\n                    <Card.Footer\n                      className=\"text-muted\"\n                      style={{ fontSize: \"10px\" }}\n                    >\n                      @{blog.author.name} {moment().startOf(\"hour\").fromNow()}\n                    </Card.Footer>\n                  </Card.Body>\n                </Card>\n              ))}\n            </CardDeck>\n          ) : (\n            <p>There are no blogs</p>\n          )}\n        </>\n      )}\n    </Container>\n  );\n};\n\nexport default HomePage;\n","/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/LoginPage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/RegisterPage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/Admin/BlogListPage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/Admin/FriendListPage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/Admin/ProfilePage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/Admin/SideMenu.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/reducers/blog.reducer.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/reducers/auth.reducer.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/reducers/route.reducer.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/reducers/user.reducer.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/components/PaginationBar.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/actions/blog.actions.js",["152","153","154"],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/actions/auth.actions.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/actions/route.actions.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/constants/blog.constants.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/constants/auth.constants.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src