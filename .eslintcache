   dispatch(blogActions.blogsRequest(pageNum));\n  }, [dispatch, pageNum]);\n\n  useEffect(() => {\n    if (redirectTo) {\n      history.push(redirectTo);\n      dispatch(routeActions.removeRedirectTo());\n    }\n  }, [dispatch, redirectTo]);\n  const handleWriteBlog = () => {\n    dispatch(routeActions.redirect(`/blog/add`));\n  };\n\n  return (\n    <Container>\n      <Card className=\"text-center\">\n        <Card.Body>\n          <Card.Title>SOCIAL BLOG</Card.Title>\n          <Card.Text>Write about your amazing things</Card.Text>\n          {isAuthenticated ? (\n            <Button variant=\"primary\" onClick={handleWriteBlog}>\n              Write Blog\n            </Button>\n          ) : (\n            <></>\n          )}\n        </Card.Body>\n      </Card>\n      <PaginationBar\n        pageNum={pageNum}\n        setPageNum={setPageNum}\n        totalPageNum={totalPageNum}\n      ></PaginationBar>\n      {loading ? (\n        <div className=\"text-center\">\n          <ClipLoader color=\"red\" size={150} loading={true} />\n        </div>\n      ) : (\n        <>\n          {blogs.length > 0 ? (\n            <CardDeck>\n              {blogs.map((blog) => (\n                <Card\n                  className=\" mt-5 d-flex flex-wrapped\"\n                  style={{ width: \"200px\", flex: \"0 0 auto\", height: \"200px\" }}\n                >\n                  {blog.images?.length > 0 && (\n                    <Card.Img variant=\"top\" src={blog.images[0]} />\n                  )}\n                  <Card.Body>\n                    <Card.Title>{blog.title}</Card.Title>\n\n                    <Card.Footer\n                      className=\"text-muted\"\n                      style={{ fontSize: \"10px\" }}\n                    >\n                      @{blog.author.name} {moment().startOf(\"hour\").fromNow()}\n                    </Card.Footer>\n                  </Card.Body>{\" \"}\n                </Card>\n              ))}\n            </CardDeck>\n          ) : (\n            <p>There are no blogs</p>\n          )}\n        </>\n      )}\n    </Container>\n  );\n};\n\nexport default HomePage;\n","/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/LoginPage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/RegisterPage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/Admin/BlogListPage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/Admin/FriendListPage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/Admin/ProfilePage.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/pages/Admin/SideMenu.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/reducers/blog.reducer.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/reducers/auth.reducer.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/reducers/route.reducer.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/reducers/user.reducer.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/components/PaginationBar.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/actions/blog.actions.js",["148","149","150"],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/actions/auth.actions.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/actions/route.actions.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/constants/blog.constants.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/constants/auth.constants.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/constants/route.constants.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/constants/user.constants.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/apiService.js",[],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/components/ReviewCard.js",["151"],"/Users/quyen/coderschool/week6/lifeblog/life-blog/src/redux/actions/user.actions.js"